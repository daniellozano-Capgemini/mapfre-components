{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mapfre-components": {
      "projectType": "library",
      "root": "projects/mapfre-components",
      "sourceRoot": "projects/mapfre-components/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/mapfre-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mapfre-components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/mapfre-components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mapfre-components/src/test.ts",
            "tsConfig": "projects/mapfre-components/tsconfig.spec.json",
            "karmaConfig": "projects/mapfre-components/karma.conf.js"
          }
        }
      }
    },
    "mapfre-demo": {
      "projectType": "library",
      "root": "projects/mapfre-demo",
      "sourceRoot": "projects/mapfre-demo/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/mapfre-demo/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mapfre-demo/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/mapfre-demo/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mapfre-demo/src/test.ts",
            "tsConfig": "projects/mapfre-demo/tsconfig.spec.json",
            "karmaConfig": "projects/mapfre-demo/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
